graph1={
    '1':set(['4','6']),
    '2':set(['3','5','6']),
    '3':set(['2','5']),
    '4':set(['1']),
    '5':set(['2','3']),
    '6':set(['1','2'])
    }
def dfs(graph,node,visited):
    if node not in visited:
        visited.append(node);
        for n in graph[node]:
            dfs(graph,n,visited);
        return visited
visited=dfs(graph1,'1',[])
print(visited)
